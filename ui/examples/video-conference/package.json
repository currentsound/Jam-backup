{
  "name": "jam-video-conference",
  "version": "0.1.0",
  "description": "Jam Video Conference Component",
  "license": "MIT",
  "author": "team@jam.systems",
  "keywords": [
    "video",
    "audio",
    "conference"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/jam-video-conference.umd.js",
  "module": "./dist/jam-video-conference.es.js",
  "exports": {
    ".": {
      "import": "./dist/jam-video-conference.es.js",
      "require": "./dist/jam-video-conference.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es2020": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "dependencies": {
    "jam-core-react": "^0.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "use-resize-observer": "^9.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^2.2.0",
    "eslint": "^8.41.0",
    "eslint-plugin-react": "^7.32.2",
    "jsdom": "^22.0.0",
    "sass": "^1.56.1",
    "vite": "^3.2.3"
  }
}
